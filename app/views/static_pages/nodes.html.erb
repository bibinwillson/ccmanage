


<% provide(:title, 'NODES') %>

  <form class="well">  
  <label>Node IP</label>
 <input type="text" class="span3" placeholder= "Enter The Server IP" />
 <label>User Name</label>
 <input type="text" class="span3" placeholder= "Enter The User Name" />
 <label>Password</label>
 <input type="text" class="span3" placeholder= "Enter Password If Any" />
 <div class="control-group">  
    <label class="control-label" for="fileInput">Select The Server Key</label>  
      <div class="controls">  
          <input class="input-file" id="fileInput" type="file">  
      </div>  
  </div>  
  <div class="form-actions">  
            <button type="submit" class="btn btn-primary">Save changes</button>  
            <button class="btn">Cancel</button>  
          </div>  
  </form>